{
	"manifest_version": 2,
	"name": "Vidal Search",
	"version": "0.1",

	"description": "L'information VIDAL à portée de main",

	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"js": [
			"jquery-2.1.3.min.js", 
			"jquery.highlight.js",
			"vidal-search.js"
		],
		"css": ["vidal-search.css"]			
	}],
	"web_accessible_resources": ["*.png"],
	"permissions": [
    		"http://*.vidal.fr/"
  	]
}
